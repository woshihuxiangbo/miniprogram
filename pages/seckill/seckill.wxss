 .rob-progree {
  background-color: #fff;
  border-bottom: 1rpx solid #f2f2f2;
  margin: 0 25rpx;
  height: 74rpx;
  line-height: 74rpx;
  display: flex;
}

 .rob-progree view {
  flex: 1;
  font-size: 34rpx;
  color: #333;
  text-align: center;
}

 .rob-progree .rob1 {
  font-weight: 800;
}

 .seck-time .current-time {
  margin-top: 36rpx;
  text-align: center;
  font-size: 34rpx;
  color: #333;
}

 .seck-time .count-down {
  text-align: center;
  margin-bottom: 40rpx;
}

 .seck-time .count-down .val {
  font-size: 24rpx;
  color: #333;
}

 .seck-time .count-down .hour, .seckill .seck-time .count-down .minute,
.seckill .seck-time .count-down .second {
  font-size: 20rpx;
  color: #fff;
  background-color: #de635c;
  padding: 5rpx 15rpx;
  border-radius: 15rpx;
  margin: 20rpx 8rpx;
}

.seckill .tips {
  height: 50rpx;
  display: flex;
  align-items: center;
  position: relative;
  background-color: #fff;
  border-bottom: 2rpx solid #f2f2f2;
}

.seckill .tips .inform {
  height: 37rpx;
  line-height: 37rpx;
  padding: 0 18rpx;
  border-radius: 0 18rpx 18rpx 0;
  box-shadow: 1rpx 2rpx 10rpx #d6bfc3;
  font-size: 22rpx;
  color: #de635c;
}

.seckill .tips .recommend {
  position: absolute;
  font-size: 24rpx;
  color: #625f5f;
  right: 60rpx;
  top: 10rpx;
}

.seckill .swiper3 .swiper {
  height: 365rpx;
}

.seckill .swiper3 .swiper .item3 {
  width: 100%;
  height: 365rpx;
  display: flex;
  padding: 0 20rpx;
  box-sizing: border-box;
}

.seckill .swiper3 .swiper .item3 .an {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}

.seckill .swiper3 .swiper .item3 .an image {
  width: 208rpx;
  height: 202rpx;
}

.seckill .swiper3 .swiper .item3 .an .text {
  width: 208rpx;
}

.seckill .swiper3 .swiper .item3 .an .text view {
  height: 40rpx;
  line-height: 40rpx;
}

.seckill .swiper3 .swiper .item3 .an .text .name {
  font-size: 24rpx;
  color: #625f5f;
  font-weight: 800;
}

.seckill .swiper3 .swiper .item3 .an .text .price {
  font-size: 24rpx;
  color: #f00;
  font-weight: 800;
}

.seckill .goodslist {
  padding-top: 20rpx;
  background-color: #f2f2f2;
  margin: 0 30rpx;
  margin-top: 20rpx;
}

.seckill .goodslist .gooditem {
  background-color: #fff;
  padding: 30rpx 0;
  display: flex;
}

.seckill .goodslist .gooditem .imgbox {
  width: 220rpx;
  height: 220rpx;
}

.seckill .goodslist .gooditem .imgbox .goodpic {
  width: 100%;
  height: 100%;
}

.seckill .goodslist .gooditem .goodinfo {
  flex: 1;
  padding-left: 30rpx;
}

.seckill .goodslist .gooditem .goodinfo .money1,
.seckill .goodslist .gooditem .goodinfo .money2,
.seckill .goodslist .gooditem .goodinfo .other {
  margin-top: 10rpx;
}

.seckill .goodslist .gooditem .goodinfo .name {
  font-size: 24rpx;
  color: #333;
  font-weight: 600;
}

.seckill .goodslist .gooditem .goodinfo .money1 {
  margin-top: 40rpx;
}

.seckill .goodslist .gooditem .goodinfo .money1 .it-much {
  font-size: 34rpx;
  color: #f00;
  font-weight: bold;
}

.seckill .goodslist .gooditem .goodinfo .money1 .it-seck {
  font-size: 20rpx;
  color: #fff;
  padding: 10rpx 25rpx;
  border-radius: 10rpx;
  background-color: #de635c;
  float: right;
}

.seckill .goodslist .gooditem .goodinfo .money2 {
  margin-top: 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.seckill .goodslist .gooditem .goodinfo .money2 .old-price {
  font-size: 24rpx;
  color: #625f5f;
  text-decoration: line-through;
  padding: 0 20rpx;
}

.seckill .goodslist .gooditem .goodinfo .money2 .jingdu {
  height: 28rpx;
}

.seckill .goodslist .gooditem .goodinfo .money2 .jingdu .percentage {
  font-size: 20rpx;
  height: 28rpx;
  line-height: 28rpx;
  color: #625f5f;
  float: left;
  margin-right: 10rpx;
}

.seckill .goodslist .gooditem .goodinfo .money2 .jingdu .jinduline {
  margin-top: 10rpx;
  width: 140rpx;
  float: left;
  border: 1rpx solid #f51f13;
}
