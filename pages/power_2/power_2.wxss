.box .tit {
  font-size: 48rpx;
  font-weight: bold;
  color: #de635c;
  position: absolute;
  top: 30rpx;
  left: 50%;
  transform: translateX(-50%);
}

.box .p {
  width: 100%;
  position: absolute;
  bottom: 80rpx;
  left: 50%;
  transform: translateX(-50%);
}

.box .p .top {
  font-size: 28rpx;
  font-weight: bold;
}

.box .p .top .l {
  color: #999;
}

.box .p .top .r {
  color: #de635c;
  font-size: 34rpx;
}

.box .p .bottom {
  font-size: 28rpx;
  font-weight: bold;
  color: #de635c;
  margin-top: 20rpx;
}
.term .content {
  width: 600rpx;
  margin: 0 auto;
}

.term .content .txt {
  padding-bottom: 40rpx;
}

.term .content .txt::before {
  content: '';
  display: inline-block;
  width: 20rpx;
  height: 20rpx;
  margin-right: 20rpx;
  background: #fd887d;
  border-radius: 50%;
}
