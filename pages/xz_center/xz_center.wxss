page {
  background: #f6f7f8;
}

.partner-info {
  height: 215rpx;
  background-color: #de635d;
  position: relative;
}

.partner-info .head-box {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin: 0 20rpx;
  padding-top: 20rpx;
}

.partner-info .head-box .head-pic {
  width: 86rpx;
  height: 86rpx;
  border-radius: 50%;
  flex-shrink: 0;
  margin-right: 10rpx;
}

.partner-info .head-box .info {
  position: relative;
}

.partner-info .head-box .info .top {
  display: flex;
  align-items: center;
  line-height: 40rpx;
}

.partner-info .head-box .info .top .name {
  font-size: 32rpx;
  max-width: 300rpx;
  color: #fff;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.partner-info .head-box .info .top .type {
  font-size: 24rpx;
  white-space: nowrap;
  padding: 0 10rpx;
  line-height: 30rpx;
  border-radius: 8rpx;
  border: 2rpx solid #fff100;
  color: #fff100;
  text-align: center;
  position: absolute;
  left: 100%;
  margin-left: 30rpx;
}

.partner-info .head-box .info .bottom {
  font-size: 24rpx;
  color: #fff;
  line-height: 40rpx;
  text-align: left;
}

.head_left {
  display: flex;
  align-items: center;
  /* width: 60%; */
  height: 86rpx;
  padding: 0;
  margin: 0;
  background-color: transparent;
  overflow: visible;
}

.partner-info .head-box .service, .partner-info .head-box .relation {
  font-size: 24rpx;
}

.partner-info .head-box .service {
  margin: 0;
  margin-right: 20rpx;
  width: 80rpx;
  height: 42rpx;
  padding: 0;
  line-height: 42rpx;
  border-radius: 22rpx;
  color: #333;
  text-align: center;
  background-color: #fff;
}

.partner-info .head-box .relation {
  display: flex;
  align-items: center;
  height: 44rpx;
  padding: 0 15rpx;
  border-radius: 22rpx;
  background-color: #fff;
}

.partner-info .head-box .relation .word {
  font-size: 24rpx;
}

.partner-info .head-box .relation .icon {
  width: 20rpx;
  height: 20rpx;
}

.partner-info .money-card {
  box-sizing: border-box;
  width: 702rpx;
  height: 176rpx;
  position: absolute;
  left: 50%;
  margin-left: -351rpx;
  bottom: -88rpx;
  background-color: #fff;
  border-radius: 25rpx;
  box-shadow: 0 0 15rpx #ccc;
  padding: 0 32rpx;
}

.partner-info .money-card .pic {
  width: 34rpx;
  height: 34rpx;
  position: absolute;
  top: 70rpx;
}

.partner-info .money-card .message-pic {
  right: 30rpx;
}

.partner-info .money-card .meney-data {
  display: flex;
  width: 550rpx;
  margin: 0 auto;
  height: 176rpx;
}

.partner-info .money-card .d1 {
  flex: 1;
  font-size: 28rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  color: #333;
}

.partner-info .money-card .d1 .val .much {
  color: #de635d;
  margin-right: 10rpx;
}

.partner-info .money-card .d1 .name {
  margin-top: 15rpx;
}

.swiper-box {
  height: 300rpx;
  margin-top: 122rpx;
}

.swiper-box .swiper .slide-image {
  width: 100%;
  height: 295rpx;
  padding: 0 25rpx;
  box-sizing: border-box;
}

.order-form {
  margin-top: 28rpx;
  border-top: 2rpx solid #eee;
  padding: 0 38rpx;
}

.order-form .title {
  margin-top: 34rpx;
  height: 42rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.order-form .title .left, .order-form .title .option {
  display: flex;
  align-items: center;
}

.order-form .title .left .tit-icon {
  width: 64rpx;
  height: 64rpx;
}

.order-form .title .left .tit-name {
  font-size: 30rpx;
  color: #333;
}

.order-form .title .option .tit-opt {
  font-size: 24rpx;
  color: #666;
}

.order-form .title .option .return-icon {
  width: 30rpx;
  height: 30rpx;
  margin: 0 10rpx;
}

.order-form .container {
  display: flex;
  margin: 34rpx 0;
}

.order-form .container .item {
  width: 25%;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}

.order-form .container .item .item-icon {
  height: 64rpx;
  width: 64rpx;
}

.order-form .container .item .item-name {
  margin-top: 6rpx;
  font-size: 24rpx;
  color: #333;
}

.order_status_icon {
  width: 30rpx;
  height: 30rpx;
  border: 1rpx solid #ff0c00;
  color: #ff0c00;
  font-size: 18rpx;
  position: absolute;
  right: 20rpx;
  top: -10rpx;
  border-radius: 50%;
  text-align: center;
  line-height: 30rpx;
  background: white;
}

.order-form .container .item {
  position: relative;
}

.navn {
  margin: 118rpx 30rpx 0;
  padding: 10rpx 0;
  background: #fff;
  box-shadow: 0 0 15rpx #ccc;
  border-radius: 25rpx;
  display: flex;
  flex-wrap: wrap;
}

.navn .navitem {
  width: 33.3%;
  text-align: center;
  margin: 30rpx auto;
  padding: 0;
  background: transparent;
  font-size: 30rpx;
  color: #333;
}

.navn .icon {
  display: block;
  width: 64rpx;
  height: 64rpx;
  margin: 0 auto;
}

.upgrade .icon {
  width: 64rpx;
  height: 64rpx;
  vertical-align: middle;
  padding-right: 10rpx;
  flex-shrink: 0;
  flex-grow: 0;
}

.upgrade {
  margin: 30rpx;
  padding: 20rpx 0;
  background: #fff;
  border-radius: 4rpx;
  box-shadow: 0 0 15rpx #ccc;
  font-size: 28rpx;
  color: #333;
  text-align: center;
  line-height: 64rpx;
}

.upgrade .flx {
  margin-top: 20rpx;
  display: flex;
  justify-content: space-around;
}

.upgrade .flx .alt {
  color: #de635c;
}

.upgrade .flx>view {
  flex: 1;
}
